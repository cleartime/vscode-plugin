{
	"name": "cleartime",
	"publisher": "gxx",
	"displayName": "",
	"description": "cleartime",
	"version": "0.0.1",
	"repository": "https://github.com/cleartime/vscode-plugin.git",
	"engines": {
		"vscode": "^1.38.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.helloWorld",
				"title": "Hello Worl2d"
			}
		],
		"menus": {
			"editor/context": [{
				"when": "resourceLangId == javascript",
				"command": "markdown.showPreview",
				"alt": "markdown.showPreviewToSide",
				"group": "navigation"
			}]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.38.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.2.0"
	}
}
